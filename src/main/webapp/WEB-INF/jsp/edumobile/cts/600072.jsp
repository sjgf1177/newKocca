<script>

var HOME_PATH = window.HOME_PATH || '.';

var cityhall = new naver.maps.LatLng(37.5904571, 127.0445058),
    map = new naver.maps.Map('map', {
        center: cityhall,
        zoom: 13
    }),
    marker = new naver.maps.Marker({
        map: map,
        position: cityhall
    });

var contentString = [
        '<div class="iw_inner" style="padding:0 10px;">',
        '   <h4 style="font-size:14px; text-align:center;">콘텐츠인재캠퍼스</h4>',
        '</div>'
    ].join('');

var infowindow = new naver.maps.InfoWindow({
    content: contentString
});

naver.maps.Event.addListener(marker, "click", function(e) {
    if (infowindow.getMap()) {
        infowindow.close();
    } else {
        infowindow.open(map, marker);
    }
});

infowindow.open(map, marker);

</script>