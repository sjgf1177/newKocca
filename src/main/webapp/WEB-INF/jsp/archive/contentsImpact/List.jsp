<script type="text/javascript">

	// url 에서 parameter 추출
	function getParam(sname) {
	    var params = location.search.substr(location.search.indexOf("?") + 1);
	    var sval = "";
	    params = params.split("&");
	    for (var i = 0; i < params.length; i++) {
	        temp = params[i].split("=");
	        if ([temp[0]] == sname) { sval = temp[1]; }
	    }
	    return sval;
	}
	
	var v_pageNum = getParam("pageNum");
	
	pageControl();
	
	// 페이지 노출
	function pageControl(){
		
		if(v_pageNum == '1'){

			$("#listPage1").show();
			$("#listPage2").hide();
			$("#pageing1").show();
			$("#pageing2").hide();
			
		} else if(v_pageNum == '2'){
			
			$("#listPage2").show();
			$("#listPage1").hide();
			$("#pageing2").show();
			$("#pageing1").hide();
			
		} else {
			
			$("#listPage1").show();
			$("#listPage2").hide();
			$("#pageing1").show();
			$("#pageing2").hide();			
			
		}
	}
	
	// 페이지 이동
	function pageMove(num){
		
		var v_num = num
		
		if(v_num == '1'){
			location.href = "/edu/main/contents.do?menuNo=500231&pageNum=1";
		} else if(v_num == '2'){
			location.href = "/edu/main/contents.do?menuNo=500231&pageNum=2";
		}
		
	}


</script>