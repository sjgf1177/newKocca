    <http pattern="/files/**" security="none"/>
    <http pattern="/js/**" security="none"/>
    <http pattern="/bos/new/**" security="none"/>
    <http pattern="/bos/js/**" security="none"/>
    <http pattern="/bos/css/**" security="none"/>
