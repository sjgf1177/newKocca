$.fn.outerHTML = function () { //rsg 20170420am1141
    var el = $(this);
    if (!el[0]) return "";

    var _html = "";
    if (1 == $(this).length) {
        if (el[0].outerHTML) {
            return el[0].outerHTML;
        } else {
            var content = el.wrap('<p/>').parent().html();
            el.unwrap();
            return content;
        }
    } else if (1 < $(this).length) {
        $(this).each(function () {
            var el = $(this);
            if (!el[0]) return;
            if (typeof (el[0]).outerHTML === "undefined") {
                return;
            }
            _html += (el[0]).outerHTML;
        });
    }
    return _html;
};

$.fn.replaceWith = function (html) { //rsg 20170420pm0541

    if (!this) return;

    if (this.outerHTML) {
        var $newEle = $(html);
        //outer
        var _this = this;
        $.map($(this).attr(), function (value, index) { //init
            $(_this).removeAttr(index + "");
        });
        var arrAtt = $.map($newEle.attr(), function (value, index) { //init
            return [{value: value, name: index}];
        });
        for (var a = 0; a < arrAtt.length; a++) {
            $(_this).attr(arrAtt[a].name, arrAtt[a].value);
        }

        //inner
        $(this).html($newEle.html());
    }
    else {
        return;
    }
};

$.fn.transFieldValue = function (item) { //rsg 20170516pm0319
    // console.log( "transFieldValue call" );

    var el = $(this);
    if (!el[0]) return;
    var htmlItem = $(this).outerHTML();
    var idxSearch = 0;

    while (htmlItem.indexOf("{{", idxSearch) != -1) {
        var idxStart = htmlItem.indexOf("{{", idxSearch);
        var idxEnd = htmlItem.indexOf("}}", idxSearch);
        var strField = htmlItem.substring(idxStart + 2, idxEnd);
        var value = item[strField];

        var idxCemi = strField.indexOf(":"); //문자열 길이 반영
        if (idxCemi != -1) {
            var strFieldName = strField.substring(0, idxCemi);
            var nLen = strField.substring(idxCemi + 1, strField.length);
            value = item[strFieldName].slice();
            value = value.substring(0, nLen);
            if (nLen < item[strFieldName].length) value = value + "..";
        }
        if (typeof value === "undefined") {
            idxSearch = idxEnd + 4;
            continue;
        }
        while (htmlItem.indexOf("{{" + strField + "}}") != -1)
            htmlItem = htmlItem.replace("{{" + strField + "}}", value);
    }
    $(this).replaceWith(htmlItem);
    return $(this);
};

$.fn.transRemoveComment = function () { //rsg 20170419pm0232
    var el = $(this);
    if (!el[0]) return;
    var htmlItem = $(this).outerHTML();

    while (htmlItem.indexOf("<!--") != -1) {
        var idxStart = htmlItem.indexOf("<!--");
        var idxEnd = htmlItem.indexOf("-->");
        var strField = htmlItem.substring(idxStart, idxEnd + 3);
        htmlItem = htmlItem.replace(strField, "");
    }
    // console.log( htmlItem );
    $(this).replaceWith(htmlItem);
};

